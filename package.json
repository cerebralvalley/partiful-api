{
  "name": "partiful-api",
  "version": "0.1.0",
  "description": "Unofficial Partiful API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebralvalley/partiful-api.git"
  },
  "keywords": [
    "boilerplate",
    "typescript"
  ],
  "author": "Ivan Porollo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerebralvalley/partiful-api/issues"
  },
  "homepage": "https://github.com/cerebralvalley/partiful-api#readme",
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.35.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "2.6.1"
  }
}
